    DEFAULT REL
    SECTION .text

;------------------------------------------------------------------------------
; void
; __cdecl
; AsmWriteMm4 (
;    UINT64   Value
;   );
;------------------------------------------------------------------------------
global AsmWriteMm4
AsmWriteMm4:
    ;
    ; 64-bit MASM doesn't support MMX instructions, so use opcode here
    ;
    DB      0x48, 0xf, 0x6e, 0xe1
    ret

